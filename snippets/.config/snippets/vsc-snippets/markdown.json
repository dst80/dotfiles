{
    "insert_code_chunk": {
        "prefix": "codesec",
        "body": [
            "```{r$1}",
            "$2",
            "```\n",
            "$0"
        ],
        "description": "insert a code chunk, default to R language"
    },
    "level_one_header" : {
        "prefix": "#",
        "body": "# ${0}",
        "description": "create level 1 header"
    },
    "level_two_header" : {
        "prefix": "##",
        "body": "## ${0}",
        "description": "create level 1 header"
    },
    "level_tree_header" : {
        "prefix": "###",
        "body": "### ${0}",
        "description": "create level 1 header"
    },
    "italic": {
        "prefix":"ic",
        "body": "*${TM_SELECTED_TEXT}${1}*"
    },
    "bold": {
        "prefix":"bf",
        "body": "**${TM_SELECTED_TEXT}${1}**"
    },
    "italic-bold": {
        "prefix":"ibf",
        "body": "***${TM_SELECTED_TEXT}${1}***"
    },
    "include_graphics": {
        "prefix": "figsec",
        "body": [
            "```{r ${1:<label>}, echo='FALSE', out.width='${2:100}%', fig.cap='$3'}",
            "knitr::include_graphics('$4')",
            "```\n",
            "$5"
        ],
        "description": "use `knitr::include_graphics()` to insert an image"
    },
    "general_reference": {
        "prefix": "ref",
        "body": [
            "\\@ref($1) $0"
        ],
        "description": "general reference; inserts '`\\@ref()`'"
    },
    "general_reference_alias": {
        "prefix": "@ref",
        "body": [
            "\\@ref($1) $0"
        ],
        "description": "general reference; inserts '`\\@ref()`'"
    },
    "section_reference": {
        "prefix": "refsec",
        "body": [
            "Section \\@ref($1) $0"
        ],
        "description": "section reference; inserts 'Section `\\@ref()`'"
    },
    "section_reference_alias": {
        "prefix": "sec",
        "body": [
            "Section \\@ref($1) $0"
        ],
        "description": "section reference; inserts 'Section `\\@ref()`'"
    },
    "figure_reference": {
        "prefix": "reffig",
        "body": [
            "Figure \\@ref(fig:$1) $0"
        ],
        "description": "figure reference; inserts 'Figure `\\@ref(fig:)`'"
    },
    "figure_reference_alias": {
        "prefix": "@fig",
        "body": [
            "Figure \\@ref(fig:$1) $0"
        ],
        "description": "figure reference; inserts 'Figure `\\@ref(fig:)`'"
    },
    "table_reference": {
        "prefix": "reftab",
        "body": [
            "Table \\@tab($1) $0"
        ],
        "description": "table reference; inserts 'Table `\\@ref(tab:)`'"
    },
    "table_reference_alias": {
        "prefix": "\\@tab",
        "body": [
            "Table \\@tab($1) $0"
        ],
        "description": "table reference; inserts 'Table `\\@ref(tab:)`'"
    },
    "Insert task list": {
        "prefix": "task",
        "body": [
            "- [${1| ,x|}] ${2:text}",
            "${0}"
        ],
        "description": "Insert task list"
    },
